FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
        type            zeroGradient;
    }

    Outlet
    {
        type            fixedValue;
	value		uniform 0;
    }

    frontback
    {
        type            fixedFluxPressure;
    }
    top
    {
        type            cyclic;
    }
    bottom
    {
        type            cyclic;
    }
    support
    {
        type            fixedValue;
	value		uniform 0;
    }
}

